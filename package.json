{
  "name": "chomex",
  "version": "0.0.53",
  "description": "Chrome Extension Routing Kit",
  "main": "lib/chomex.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
    "release": "./node_modules/.bin/coffee ./scripts/release.coffee",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/otiai10/chomex.git"
  },
  "keywords": [
    "chrome"
  ],
  "author": "otiai10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/otiai10/chomex/issues"
  },
  "homepage": "https://github.com/otiai10/chomex#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "coffee-script": "^1.12.3",
    "eslint": "^3.15.0",
    "jest-cli": "^18.1.0",
    "webpack": "^2.2.1"
  },
  "jest": {
    "verbose": true,
    "testRegex": "tests/.*-test\\.js$",
    "unmockedModulePathPatterns": [
      "<rootDir>/src/chomex"
    ],
    "setupFiles": [
      "./tests/mock/runtime.js"
    ]
  }
}